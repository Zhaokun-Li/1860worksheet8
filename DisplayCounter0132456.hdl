CHIP DisplayCounter0132456{
    IN inc, reset;
    OUT a,b,c,d,e,f,g;

    PARTS:
    Register(in=nextValue,load=load,out=currentValue);
    Inc16(in=currentValue,out=incrementedValue);
    And(a=inc,b=reset,out=load);
    Mux16(a=false,b=incrementedValue,sel=reset,out=nextValue);
    Splitter(in=currentValue, out0=value0, out1=value1, out2=value2, out3=value3);
    Decoder201840925(A=value3,B=value2,C=value1,D=value0, a=a, b=b, c=c, d=d, e=e, f=f, g=g);

}